{
  "presets": [
    "react",
    "stage-2",
    [
      "env",
      {
        "targets": {
          "browsers": [">= 5%", "ie >= 11"]
        }
      }
    ]
  ],
  "plugins": [
    "transform-flow-comments"
  ],
  "env": {
    "production": {
      "presets": [
        "flow"
      ],
      "plugins": [
        "remove-comments"
      ],
      "compact": true
    }
  }
}
